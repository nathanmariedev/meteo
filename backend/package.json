{
  "name": "nestjs-wca-meteo",
  "version": "0.0.1",
  "description": "A Nestjs starter kit, batteries included",
  "author": "Renaud Lataguerra",
  "license": "MIT",
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "scripts": {
    "knex": "DOTENV_CONFIG_PATH=config.env node -r dotenv/config node_modules/.bin/knex",
    "build": "rimraf dist && tsc -p tsconfig.build.json && npm run copyNonTsFilesToDist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:ts-node": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" \"npm run copyNonTsFilesToDist\" \"npm run watchSqlFiles\" \"npm run watchNjkFiles\"",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "DOTENV_CONFIG_PATH=config.env node -r dotenv/config node_modules/.bin/jest --config ./test/jest-e2e.json --forceExit --detectOpenHandles --maxWorkers=10",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "setname": "node setname.js",
    "copyNonTsFilesToDist": "copyfiles -u 1 \"./src/**/*.sql\" ./dist && copyfiles -u 1 \"./src/**/*.njk\" ./dist",
    "watchSqlFiles": "onchange \"./src/**/*.sql\" -- copyfiles -u 1 \"./src/**/*.sql\" ./dist",
    "watchNjkFiles": "onchange \"./src/**/*.njk\" -- copyfiles -u 1 \"./src/**/*.njk\" ./dist"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.11",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.11",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/typeorm": "9.0.1",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@types/passport": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.0",
    "helmet": "^5.0.1",
    "joi": "^17.4.1",
    "knex": "1.0.7",
    "knex-case-converter-plugin": "^0.1.1",
    "morgan": "^1.10.0",
    "nunjucks": "^3.2.3",
    "onchange": "7.1.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.3.14",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chalk": "^2.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "figlet": "^1.5.0",
    "husky": "^7.0.2",
    "inquirer": "^8.1.2",
    "jest": "^27.0.6",
    "lint-staged": "^12.1.6",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "replace-in-file": "^6.2.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.10.1",
    "wait-on": "^6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "npm run format",
      "npm run lint"
    ]
  }
}
